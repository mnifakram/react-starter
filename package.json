{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Boilerplate for react and webpack",
  "main": "index.js",
  "repository": "https://github.com/mnifakram/react-starter",
  "scripts": {
    "start": "env-cmd cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --no-info --open",
    "build": "env-cmd cross-env NODE_ENV=production webpack",
    "format": "prettier --write 'packages/**/*.js'",
    "eslint": "eslint --fix .",
    "test": "jest --updateSnapshot"
  },
  "lint-staged": {
    "*.{js,jsx,scss,css,md}": [
      "prettier --write --single-quote",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "author": "Akram Mnif",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.8.3",
    "babel-plugin-transform-react-pure-class-to-function": "1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "core-js": "3.6.4",
    "dotenv": "^8.1.0",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.12"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.8.3",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-syntax-import-meta": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.8.3",
    "@babel/register": "7.8.3",
    "@babel/runtime": "7.8.4",
    "@hot-loader/react-dom": "^16.9.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-console-source": "^2.0.4",
    "babel-plugin-groundskeeper-willie": "^1.3.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-directive": "^0.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "browserslist": "4.8.2",
    "clean-webpack-plugin": "3.0.0",
    "connect-history-api-fallback": "1.6.0",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "dotenv-webpack": "^1.7.0",
    "env-cmd": "^10.0.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-watch": "6.0.1",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-prefix-plugin": "^1.1.0",
    "husky": "4.2.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-css-modules-transform": "4.0.0",
    "jest-enzyme": "7.1.2",
    "jest-fetch-mock": "2.1.2",
    "jsdom": "16.0.1",
    "koa-connect": "2.0.1",
    "lint-staged": "10.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "npm-check-updates": "3.1.21",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "script-ext-html-webpack-plugin": "2.1.4",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.3.4",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.10",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.2",
    "webpack-redis-plugin": "^1.0.0",
    "webpack-s3-uploader": "1.3.3"
  }
}
